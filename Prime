#include <stdio.h>
int Check(int,int);

int main()
{
    int n;
    printf("Enter the number:");
    scanf("%d", &n);
    int c=Check(n,n/2);
    if(c==1)
    printf("Yes Prime");
    else
    printf("Not prime");
}

int Check(int n,int i)
{
    if(i==1)
    return(1);
    else if( n%i==0)
    return(0);
    else
    return(n,i--);
}
